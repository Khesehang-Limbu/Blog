<%- include("./partials/header")-%>
<h1>Home</h1>
<p> <%= homeStartingContent %> </p>

<% posts.forEach((post)=>{ %>
    <% let url = "/posts/" + post._id; %>
    <h2> <%= post.title %> </h2> 
    <p> <%= post.content.substring(0, 100) + "..."%> <a href="<%= url %>">Read More</a></p>
<% }) %>

<%- include("./partials/footer")-%>